    "mergeBams": {
        "name_process": "mergeBams",
        "string_process": "\nprocess mergeBams {\n    input:\n    file bams\n\n    output:\n    file(\"merged.bam\")\n\n    \"\"\"\n    bams=\"${bams}\"\n    words=( \\$bams )\n    if [[ \"1\" == \"\\${#words[@]}\" ]]; then\n        cp ${bams} merged.bam\n    else\n        samtools merge merged.bam ${bams}\n    fi\n    \"\"\"\n}",
