// structure
digraph {
	node [colorscheme=pastel19 style=filled]
	mapping [label=mapping color=2 shape=box]
	generateBam [label=generateBam color=2 shape=box]
	generateBed [label=generateBed color=2 shape=box]
	removeNonStandardChrom [label=removeNonStandardChrom color=2 shape=box]
	bedToUCSCNames [label=bedToUCSCNames color=2 shape=box]
	generateBigBed [label=generateBigBed color=2 shape=box]
	generateBedGraph [label=generateBedGraph color=2 shape=box]
	bedgraphToUCSC [label=bedgraphToUCSC color=2 shape=box]
	bedSort [label=bedSort color=2 shape=box]
	generateBigWig [label=generateBigWig color=2 shape=box]
	visualizeFastq [label=visualizeFastq color=2 shape=box]
	uploadToS3 [label=uploadToS3 color=2 shape=box]
	bamIndex [label=bamIndex color=2 shape=box]
	flagstat [label=flagstat color=2 shape=box]
	geneBodyCov [label=geneBodyCov color=2 shape=box]
	picard [label=picard color=2 shape=box]
	mapping -> generateBam [label="" constraint=true]
	generateBam -> generateBed [label="" constraint=true]
	generateBam -> generateBedGraph [label="" constraint=true]
	generateBam -> bamIndex [label="" constraint=true]
	generateBam -> flagstat [label="" constraint=true]
	generateBam -> geneBodyCov [label="" constraint=true]
	generateBam -> picard [label="" constraint=true]
	generateBed -> removeNonStandardChrom [label="" constraint=true]
	removeNonStandardChrom -> bedToUCSCNames [label="" constraint=true]
	bedToUCSCNames -> generateBigBed [label="" constraint=true]
	generateBigBed -> uploadToS3 [label="" constraint=true]
	generateBedGraph -> bedgraphToUCSC [label="" constraint=true]
	bedgraphToUCSC -> bedSort [label="" constraint=true]
	bedSort -> generateBigWig [label="" constraint=true]
	generateBigWig -> uploadToS3 [label="" constraint=true]
	bamIndex -> geneBodyCov [label="" constraint=true]
}
