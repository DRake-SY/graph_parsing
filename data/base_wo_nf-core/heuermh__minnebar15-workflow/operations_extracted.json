{
    "OPERATION_1": {
        "string": "logFiles = Channel.fromPath(\"${params.logs}/**.log\")",
        "origin": [
            [
                "\"${params.logs}/**.log\"",
                "A"
            ]
        ],
        "gives": [
            [
                "logFiles",
                "P"
            ]
        ]
    },
    "OPERATION_2": {
        "string": "toArchive, toFilter = logFiles.into(2)",
        "origin": [
            [
                "logFiles",
                "P"
            ]
        ],
        "gives": [
            [
                "toArchive",
                "P"
            ],
            [
                "toFilter",
                "P"
            ]
        ]
    }
}