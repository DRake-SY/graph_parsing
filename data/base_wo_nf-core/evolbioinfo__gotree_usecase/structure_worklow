	cleanSequences [label=cleanSequences color=2 shape=box]
	alignSequences [label=alignSequences color=2 shape=box]
	concatSequences [label=concatSequences color=2 shape=box]
	cleanAlign [label=cleanAlign color=2 shape=box]
	reformatAlignment [label=reformatAlignment color=2 shape=box]
	inferTrueTree [label=inferTrueTree color=2 shape=box]
	rerootTree [label=rerootTree color=2 shape=box]
	downloadNewickTaxonomy [label=downloadNewickTaxonomy color=2 shape=box]
	pruneNCBITax [label=pruneNCBITax color=2 shape=box]
	renameNCBITaxonomy [label=renameNCBITaxonomy color=2 shape=box]
	rerootNCBITax [label=rerootNCBITax color=2 shape=box]
	annotateTree [label=annotateTree color=2 shape=box]
	compareTrees [label=compareTrees color=2 shape=box]
	uploadTree [label=uploadTree color=2 shape=box]
	getNCBIIds -> getOrthoDBIds [label="" constraint=true]
	getOrthoDBIds -> downloadSequences [label="" constraint=true]
	downloadSequences -> getMapTable [label="" constraint=true]
	getMapTable -> renameSequences [label="" constraint=true]
	renameSequences -> cleanSequences [label="" constraint=true]
	cleanSequences -> alignSequences [label="" constraint=true]
	alignSequences -> concatSequences [label="" constraint=true]
	concatSequences -> cleanAlign [label="" constraint=true]
	cleanAlign -> reformatAlignment [label="" constraint=true]
	reformatAlignment -> inferTrueTree [label="" constraint=true]
	inferTrueTree -> rerootTree [label="" constraint=true]
	inferTrueTree -> pruneNCBITax [label="" constraint=true]
	rerootTree -> annotateTree [label="" constraint=true]
	rerootTree -> compareTrees [label="" constraint=true]
	downloadNewickTaxonomy -> pruneNCBITax [label="" constraint=true]
	pruneNCBITax -> renameNCBITaxonomy [label="" constraint=true]
	renameNCBITaxonomy -> rerootNCBITax [label="" constraint=true]
	rerootNCBITax -> annotateTree [label="" constraint=true]
	rerootNCBITax -> compareTrees [label="" constraint=true]
	annotateTree -> uploadTree [label="" constraint=true]
	OPERATION_1 [label=1 color=1 shape=doublecircle]
	getOrthoDBIds -> OPERATION_1 [label="" constraint=true]
	OPERATION_2 [label=2 color=1 shape=doublecircle]
	getMapTable -> OPERATION_2 [label="" constraint=true]
}
