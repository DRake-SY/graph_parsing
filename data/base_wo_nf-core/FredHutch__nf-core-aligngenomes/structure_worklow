// structure
digraph {
	node [colorscheme=pastel19 style=filled]
	correctHeaders [label=correctHeaders color=2 shape=box]
	alignGenome [label=alignGenome color=2 shape=box]
	sortBAM [label=sortBAM color=2 shape=box]
	alignmentStats [label=alignmentStats color=2 shape=box]
	summarizeEach [label=summarizeEach color=2 shape=box]
	countReads [label=countReads color=2 shape=box]
	collectPileups [label=collectPileups color=2 shape=box]
	collectCounts [label=collectCounts color=2 shape=box]
	collectAll [label=collectAll color=2 shape=box]
	correctHeaders -> alignGenome [label="" constraint=true]
	alignGenome -> sortBAM [label="" constraint=true]
	sortBAM -> alignmentStats [label="" constraint=true]
	alignmentStats -> summarizeEach [label="" constraint=true]
	alignmentStats -> collectPileups [label="" constraint=true]
	summarizeEach -> collectAll [label="" constraint=true]
	countReads -> collectCounts [label="" constraint=true]
	collectCounts -> collectAll [label="" constraint=true]
	OPERATION_1 [label=1 color=1 shape=doublecircle]
	OPERATION_1 -> correctHeaders [label="" constraint=true]
	OPERATION_2 [label=2 color=1 shape=doublecircle]
	OPERATION_2 -> alignGenome [label="" constraint=true]
}
