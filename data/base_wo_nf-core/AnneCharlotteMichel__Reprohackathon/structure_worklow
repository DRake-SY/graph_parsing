// structure
digraph {
	node [colorscheme=pastel19 style=filled]
	getFastq [label=getFastq color=2 shape=box]
	dumpFastq [label=dumpFastq color=2 shape=box]
	getChr [label=getChr color=2 shape=box]
	concatChr [label=concatChr color=2 shape=box]
	genomeIndex [label=genomeIndex color=2 shape=box]
	mapFastq [label=mapFastq color=2 shape=box]
	samFastq [label=samFastq color=2 shape=box]
	getGenomic_features [label=getGenomic_features color=2 shape=box]
	getCount_feature [label=getCount_feature color=2 shape=box]
	deseq [label=deseq color=2 shape=box]
	plot [label=plot color=2 shape=box]
	getFastq -> dumpFastq [label="" constraint=true]
	dumpFastq -> mapFastq [label="" constraint=true]
	getChr -> concatChr [label="" constraint=true]
	concatChr -> genomeIndex [label="" constraint=true]
	genomeIndex -> mapFastq [label="" constraint=true]
	mapFastq -> samFastq [label="" constraint=true]
	mapFastq -> getCount_feature [label="" constraint=true]
	samFastq -> getCount_feature [label="" constraint=true]
	getGenomic_features -> getCount_feature [label="" constraint=true]
	getCount_feature -> deseq [label="" constraint=true]
	deseq -> plot [label="" constraint=true]
	deseq -> plot [label="" constraint=true]
	deseq -> plot [label="" constraint=true]
	OPERATION_1 [label=1 color=3 shape=doublecircle]
	OPERATION_1 -> getFastq [label="" constraint=true]
	OPERATION_2 [label=2 color=3 shape=doublecircle]
	OPERATION_2 -> getChr [label="" constraint=true]
	OPERATION_3 [label=3 color=1 shape=doublecircle]
	OPERATION_3 -> deseq [label="" constraint=true]
	OPERATION_4 [label=4 color=3 shape=doublecircle]
	OPERATION_4 -> deseq [label="" constraint=true]
