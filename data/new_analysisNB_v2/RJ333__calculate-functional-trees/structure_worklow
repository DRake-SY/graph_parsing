// structure
digraph {
	node [colorscheme=pastel19 style=filled]
	create_single_line_fasta [label=create_single_line_fasta color=2 shape=box]
	subset_gff [label=subset_gff color=2 shape=box]
	subset_protein_fasta [label=subset_protein_fasta color=2 shape=box]
	download_sequences_from_uniprot [label=download_sequences_from_uniprot color=2 shape=box]
	merge_protein_sequences [label=merge_protein_sequences color=2 shape=box]
	remove_duplicates [label=remove_duplicates color=2 shape=box]
	run_msa [label=run_msa color=2 shape=box]
	fasta_to_phylip [label=fasta_to_phylip color=2 shape=box]
	remove_special_chars_from_phylip [label=remove_special_chars_from_phylip color=2 shape=box]
	run_raxml [label=run_raxml color=2 shape=box]
	plot_tree [label=plot_tree color=2 shape=box]
	create_single_line_fasta -> subset_protein_fasta [label="" constraint=true]
	subset_gff -> subset_protein_fasta [label="" constraint=true]
	subset_protein_fasta -> merge_protein_sequences [label="" constraint=true]
	download_sequences_from_uniprot -> merge_protein_sequences [label="" constraint=true]
	merge_protein_sequences -> remove_duplicates [label="" constraint=true]
	remove_duplicates -> run_msa [label="" constraint=true]
	run_msa -> fasta_to_phylip [label="" constraint=true]
	fasta_to_phylip -> remove_special_chars_from_phylip [label="" constraint=true]
	remove_special_chars_from_phylip -> run_raxml [label="" constraint=true]
	run_raxml -> plot_tree [label="" constraint=true]
}
