// structure
digraph {
	node [colorscheme=pastel19 style=filled]
	fetchFiles [label=fetchFiles color=2 shape=box]
	readHeader [label=readHeader color=2 shape=box]
	countReads [label=countReads color=2 shape=box]
	align [label=align color=2 shape=box]
	bam_stats_qc [label=bam_stats_qc color=2 shape=box]
	merge_aligned [label=merge_aligned color=2 shape=box]
	extract_unaligned_reads [label=extract_unaligned_reads color=2 shape=box]
	extract_both_reads_unaligned [label=extract_both_reads_unaligned color=2 shape=box]
	merge_unmappedReads [label=merge_unmappedReads color=2 shape=box]
	outputMappedResults [label=outputMappedResults color=2 shape=box]
	outputMappedMetricsResults [label=outputMappedMetricsResults color=2 shape=box]
	outputUnmappedResults [label=outputUnmappedResults color=2 shape=box]
	outputUnmappedMetricsResults [label=outputUnmappedMetricsResults color=2 shape=box]
	fetchFiles -> readHeader [label="" constraint=true]
	fetchFiles -> countReads [label="" constraint=true]
	readHeader -> align [label="" constraint=true]
	countReads -> align [label="" constraint=true]
	align -> bam_stats_qc [label="" constraint=true]
	bam_stats_qc -> merge_aligned [label="" constraint=true]
	merge_aligned -> extract_unaligned_reads [label="" constraint=true]
	merge_aligned -> extract_both_reads_unaligned [label="" constraint=true]
	merge_aligned -> outputMappedResults [label="" constraint=true]
	merge_aligned -> outputMappedMetricsResults [label="" constraint=true]
	extract_unaligned_reads -> merge_unmappedReads [label="" constraint=true]
	extract_both_reads_unaligned -> merge_unmappedReads [label="" constraint=true]
	merge_unmappedReads -> outputUnmappedResults [label="" constraint=true]
	merge_unmappedReads -> outputUnmappedMetricsResults [label="" constraint=true]
}
